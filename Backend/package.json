{
  "name": "packlead",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "engines": { "node": ">=20" },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "build:runtime": "tsc -p tsconfig.runtime.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "dump:src": "tsx scripts/dump-src.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pino": "^9.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "tsx": "^4.16.2",
    "typescript": "^5.6.2"
  }
}
