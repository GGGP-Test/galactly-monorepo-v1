{
"compilerOptions": {
"target": "ES2020",
"module": "CommonJS",
"moduleResolution": "Node",
"outDir": "dist",
"rootDir": "src",
"esModuleInterop": true,
"forceConsistentCasingInFileNames": true,
"strict": true,
"skipLibCheck": true,
"noEmitOnError": true
},
"include": [
"src/index.ts",
"src/db.ts",
"src/routes/**/*.ts",
"src/connectors/cse.ts"
],
"exclude": ["node_modules"]
}
